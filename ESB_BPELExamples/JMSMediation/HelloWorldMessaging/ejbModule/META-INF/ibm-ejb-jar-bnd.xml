<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar-bnd
        xmlns="http://websphere.ibm.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://websphere.ibm.com/xml/ns/javaee http://websphere.ibm.com/xml/ns/javaee/ibm-ejb-jar-bnd_1_1.xsd"
        version="1.1">

	<message-driven name="HelloWorldMessagingMDB">
		<jca-adapter activation-spec-binding-name="jms/HelloWorldQueueActivationSpec" />
		<resource-ref name="jms/HelloWorldQueueConnectionFactory"
			binding-name="jms/HelloWorldQueueConnectionFactory" />
		<resource-env-ref name="MessageDrivenContext"
			binding-name="MessageDrivenContext" />
	</message-driven> 
</ejb-jar-bnd>
